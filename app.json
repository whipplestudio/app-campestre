{
  "expo": {
    "name": "Club Campestre Tampico",
    "slug": "club-campestre-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-campestre.png",
    "scheme": "clubcampestretampicoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mx.wipple.clubcampestretampico",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "mx.wipple.clubcampestretampico",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-campestre.png",
        "backgroundColor": "#1C1C1C"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon-campestre.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eeb6052a-5145-4e0c-bb39-f207ef31dd0e"
      }
    }
  }
}
